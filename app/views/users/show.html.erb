<%= render partial: "template/page_top_main",  locals: {guide_message: "入力画面"}%>

<div class="row">

  <div class="col s3" id="side">
    <%= render partial: "template/side_navi" %>
  </div>
  <div class="col s9">
    <div class="date-link-area col s12 l12">
      <div class='row'>
        <div class="date-link-area col s4 l4">
          <h4 id="input-date-show">
            <%= link_to user_path(current_user.id, @date.to_date-1) do %>
            <i class="material-icons">chevron_left</i>
            <% end %>

            <%= @date %>

            <%= link_to user_path(current_user.id, @date.to_date+1) do %>
            <i class="material-icons">chevron_right</i>
            <% end %>
            <i class="material-icons input-date-button">date_range</i>
            <input type='text' class='datepicker datepicker-date-link' autocomplete="off" style="display:none;" />
            <a id='date_link' href=''>
              <button id="date-link-btn" class='btn' style="display:none;">Go</button>
            </a>
          </h4>
        </div>
        <div class="todo-area col s6 l6 offset-s1 offset-l1">
          <p>ToDoリスト</p>
          <% if current_user.id == @user.id %>
          <table class="striped">
            <tbody>
              <% if current_user.projects.exists? %>
              <% current_user.projects.each do |project| %>
              <% if project.done_flg == 0 %>
              <tr>
                <td><%= project.name %></td>
                <td>目標日：<%= project.limit_date %></td>
                <td>残り日数<%= (project.limit_date.to_date - Date.today).round %>日</td>
                <%= form_with model: project, local: true do |form| %>
                <%= form.hidden_field :id, placeholder: "id", value: project.id %>
                <td>
                  <%= form.submit "完了", class: "btn submit-button  btn-small"%><br><br>
                  <% end %>
                </td>
              </tr>
              <% end %>
              <% else %>
              <% end %>
              <% end %>

            </tbody>
          </table>
          <% end %>
        </div>
      </div>
    </div>

